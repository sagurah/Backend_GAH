generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Kamar {
  ID_KAMAR Int @id @default(autoincrement())
  NO_KAMAR Int?
  JENIS_KAMAR String?
  JENIS_BED String?
  KAPASITAS Int?
  LUAS_KAMAR Int?
  FASILITAS String?
}

model Season {
  ID_SEASON Int @id @default(autoincrement())
  JENIS_SEASON String?
  TGL_MULAI DateTime?
  TGL_AKHIR DateTime?
  HARGA Float?
}
